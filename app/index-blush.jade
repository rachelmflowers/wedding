block content
  #top.banner
    .banner__img(style="background-image: url(images/loriandkeet.jpg)")
    .banner__gradient
      .banner__border
    .row
      .banner__text.default-6.small-4.xxlarge-7
        h1.big Lori <span>&</span> Keet
        h1 are getting hitched
        h2 January 1, 2016
        img(src="images/ornamen.svg", alt="Love")
  //#top.banner

  nav#topnav
    .blurred
      .blurred__wrap
    .inner
      span.mobile-nav 
        span Menu
      ul.close
        li(style={display: 'none'}): a(href="#top") &nbsp;
        li: a(href="#couple") The Couple
        li: a(href="#wedding") The Wedding
        li: a(href="#accommodations") Accommodations
        li: a(href="#gift") Registry
        li: a(href="#rsvp") RSVP
